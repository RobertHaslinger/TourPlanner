<Application x:Class="TourPlanner.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:TourPlanner"
             xmlns:converter="clr-namespace:TourPlanner.Converter"
             StartupUri="Views/BaseWindow.xaml">
    <Application.Resources>
         <converter:BoolToListHeightConverter x:Key="BoolToListHeightConverter"/>
         <converter:TourToHeightConverter x:Key="TourToHeightConverter"/>
    </Application.Resources>
</Application>
